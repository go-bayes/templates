	langid = {english},
	keywords = {national index, population, satisfaction, social indicators, subjective wellbeing},
	file = {Full Text PDF:files/3606/Cummins 등 - 2003 - Developing a National Index of Subjective Wellbein.pdf:application/pdf},
}

@article{fraser_coding_2020,
	title = {Coding responses to an open-ended gender measure in a New Zealand National sample},
	volume = {57},
	issn = {0022-4499},
	url = {https://doi.org/10.1080/00224499.2019.1687640},
	doi = {10.1080/00224499.2019.1687640},
	abstract = {In light of the methodological and ethical issues associated with using a male/female tick box to collect gender data, researchers are increasingly questioning how to measure gender inclusively in survey research. Open-ended measures afford the greatest flexibility, though whether they are practical for large-scale surveys has yet to be tested. Here, we systematically assess the feasibility of open-ended gender measures drawing on a New Zealand national probability sample (New Zealand Attitudes and Values Study, N = 15,758). We asked participants “What is your gender?” as an open-ended measure of gender, and developed a simple, cost-effective coding scheme for coding qualitative gender data. Results indicate that very few participants (n = 15) self-identified as transgender, or outside of the male/female gender binary. Moreover, we find no evidence that implementation of the open-ended measure contributes to non-response rates or panel attrition. Taken together, these results demonstrate that large-scale surveys can feasibly implement inclusive measures of gender as an alternative to binary categorical measures. Because the single-measure approach likely underestimates the number of transgender participants, however, researchers interested in identifying all participants whose gender differs from their assigned sex should utilize two-step methods, which assess gender as well as assigned sex at birth.},
	pages = {979--986},
	number = {8},
	journaltitle = {The Journal of Sex Research},
	author = {Fraser, Gloria and Bulbulia, Joseph and Greaves, Lara M. and Wilson, Marc S. and Sibley, Chris G.},
	urldate = {2022-11-27},
	date = {2020-10-12},
	pmid = {31729890},
	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/00224499.2019.1687640},
}

