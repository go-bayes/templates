	keywords = {Adult, Female, Humans, Male, Psychometrics, Reproducibility of Results, Social Environment, Social Identification, Surveys and Questionnaires, Young Adult},
}

@article{rice_short_2014,
	title = {The short form of the revised almost perfect scale},
