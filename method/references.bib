	note = {Publisher: Public Library of Science},
	keywords = {Census, Culture, Europe, Housing, Māori people, New Zealand, Racial discrimination, Religion},
	file = {Full Text PDF:files/3613/Houkamau 그리고 Sibley - 2015 - Looking Māori Predicts Decreased Rates of Home Own.pdf:application/pdf;Snapshot:files/3614/article.html:text/html},
