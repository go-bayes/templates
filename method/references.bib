	keywords = {assessment, deliberate self-harm, emotion dysregulation, emotion regulation, experiential avoidance},
	file = {Full Text PDF:files/3609/Gratz 그리고 Roemer - 2004 - Multidimensional Assessment of Emotion Regulation .pdf:application/pdf},
}

